{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      // "@src/*": ["src/*"],
      "@atoms": ["src/components/atoms"],
      "@molecules": ["src/components/molecules"],
      "@organisms": ["src/components/organisms"],
      "@router/*": ["src/router/*"],
      "@hooks": ["src/hooks"],
      "@store": ["src/store"],
      "@theme": ["src/theme"],
      "@types": ["src/types"],
      "@constants/*": ["src/constants/*"],
      "@assets/*": ["src/assets/*"],
      "@utils/*": ["src/utils/*"],
      "@screens/*": ["src/screens/*"],
    }
  }
}
